                    <div class="h-100 d-flex bg-white justify-content-center align-items-center col-md-12 col-lg-8">
                        <div class="mx-auto app-login-box col-sm-12 col-md-10 col-lg-9">

                            <h4 class="mb-0">
                                @*<span class="d-block">Welcome,</span>
                                    <span>Please sign in to your account.</span>*@
                            </h4>

                            @*<div class="divider row">
                                </div>*@
                        <div class="widget-content-right">
                            <h5 class="card-title"></h5>

                            <h5 class="card-title"><label id="prjTitle">Register</label></h5>

                           
                            <form id="formidRegister" class="needs-validation" asp-action="AddAUEUMSUser" method="post">

                                <div class="form-row">

                                    <div class="col-md-6">
                                        <label for="lblFirstName" class="">First Name</label>
                                        <div class="input-group">
                                            <input asp-for="FirstName" placeholder="" type="text" class="form-control" required maxlength="75">
                                        </div>
                                    </div>

                                    <div class="col-md-6 ">
                                        <label for="lblLastName" class="">Last Name</label>
                                        <div class="input-group">
                                            <input asp-for="LastName" placeholder="" type="text" class="form-control" required maxlength="75">
                                        </div>
                                    </div>

                                </div>

                                <div class="form-row">

                                    <div class="col-md-6 ">
                                        <label for="lblCompany" class="">Company</label>
                                        <div class="input-group">
                                          @*   <input asp-for="Company" placeholder="" type="text" class="form-control" required maxlength="100">
                                      *@   </div>
                                    </div>

                                    <div class="col-md-6">
                                        <label for="lblCity" class="">City</label>
                                        <div class="input-group">
                                            <input asp-for="City" placeholder="" type="text" class="form-control" required maxlength="100">
                                        </div>
                                    </div>


                                </div>


                                <div class="form-row">

                                    <div class="col-md-6">
                                        <label for="lblCountry" class="">Country</label>
                                        <div class="input-group">
                                          @*   <select asp-for="CountryID" class="form-control"
                                                    asp-items="@(new SelectList(Model.CountryList, "DisplayID", "DisplayValue",Model.CountryID))" required>
                                            </select> *@
                                        </div>

                                    </div>

                                    <div class="col-md-6">
                                        <label for="lblEmail" class="">Email</label>
                                        <div class="input-group">
                                            <input asp-for="Email" placeholder="" type="email" class="form-control" required maxlength="100">
                                        </div>
                                    </div>

                                </div>

                                <div class="form-row">

                                    <div class="col-md-6">
                                        <label for="lblPassword" class="">Password</label>
                                        <div class="input-group">
                                            <input asp-for="Password" placeholder="" type="password" class="form-control" required maxlength="50">
                                        </div>
                                    </div>

                                    <div class="col-md-6 mb-2">
                                        <label for="lblConfirmPassword" class="">Confirm Password</label>
                                        <div class="input-group">
                                            <input asp-for="confirmPwd" placeholder="" type="password" class="form-control" required maxlength="50">
                                        </div>
                                    </div>

                                </div>
                                <div class="form-row mb-2">
                                    <div class="col-md-12 position-relative form-group">
                                            <div class="g-recaptcha" data-sitekey="6LcNeDwjAAAAAOjmFbZsd2tkWasbz_Kto_EcUxMQ"></div>
                                     
                                    </div>
                                </div>


                                <div class="form-row mb-2">
                                    <div class="col-md-12 position-relative form-group">
                                        <div class="position-relative form-group"><label style="color:red;font-weight:normal;">@Model.errormessage</label></div>
                                    </div>
                                </div>

                                <div class="form-row mb-2">

                                    <div class="col-md-6 position-relative form-group">
                                        <button class="mt-1 btn btn-gradient-info" id="tbnSubmit" type="submit">Submit</button>
                                        <button class="mt-1 btn btn-gradient-light" id="tbnCancelSubmit" type="reset">Reset</button>

                                    </div>
                                </div>


                                <input type="hidden" asp-for="ReturnUrl" />
                            </form>
                            <div class="position-relative form-group mb-2">
                                <a asp-action="AUEUMSLogin" style="cursor: pointer;padding-right: 18px;">
                                    <i style="color:#3f6ad8;font-size:smaller;font-weight:bold;">Back to Login</i>
                                </a>
                            </div>

                        </div>


                        </div>
                    </div>
                </div>
